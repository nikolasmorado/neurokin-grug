package home

import (
	"neurokin/views/layouts"
	"neurokin/views/shared"
  t "neurokin/types"
)

templ Index(s []t.Section, t []t.Tile) {
	@layouts.Fullheight() {
		@Section1()
		@Section2(t)
    for _, i := range s { 
      @shared.General(i.Subtitle, i.Title, i.Description, i.Cta, i.Link, i.Orientation, i.BgColor)
    }
	}
}
